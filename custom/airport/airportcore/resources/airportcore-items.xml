<items xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="items.xsd">

	<collectiontypes>
		<collectiontype code="GenderList" elementtype="Gender"
			autocreate="true" generate="true" type="list" />
		<collectiontype code="SwatchColorSet"
			elementtype="SwatchColorEnum" autocreate="true" generate="true"
			type="set" />
	</collectiontypes>
	<!-- _________________________________ ENUMTYPES ____________________________________________ -->

	<enumtypes>



		<enumtype generate="true" code="SwatchColorEnum"
			autocreate="true" dynamic="true">
			<value code="BLACK" />
			<value code="BLUE" />
			<value code="BROWN" />
			<value code="GREEN" />
			<value code="GREY" />
			<value code="ORANGE" />
			<value code="PINK" />
			<value code="PURPLE" />
			<value code="RED" />
			<value code="SILVER" />
			<value code="WHITE" />
			<value code="YELLOW" />
		</enumtype>

		<enumtype generate="true" code="statePlaneEnum"
			autocreate="true" dynamic="true">
			<value code="assigned" />
			<value code="maintenance" />
			<value code="available" />
		</enumtype>



		<enumtype generate="true" code="stateFlightEnum"
			autocreate="true" dynamic="true">
			<value code="CANCELLED"></value>
			<value code="ToFLIGHT"></value>
			<value code="LANDED"></value>

		</enumtype>


	</enumtypes>






	<!-- ______________________________________ RELATIONS ______________________________________________ -->



	<relations>
		<relation localized="false" code="RouteToFlight"
			autocreate="true" generate="true">
			<sourceElement type="Route" qualifier="route"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="false" />
			</sourceElement>
			<targetElement type="Flight" qualifier="flight"
				cardinality="many" collectiontype="list">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>
		
		<relation code="AirLineToPlanes" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="airline" type="Airline"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="false" />
			</sourceElement>
			<targetElement qualifier="plane" type="Plane"
				cardinality="many" collectiontype="list">
				<modifiers read="true" write="true" search="true"
					optional="true" />
			</targetElement>
		</relation>

		<relation code="PassengerToBookings" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="passenger" type="Passenger"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="false" />
			</sourceElement>
			<targetElement qualifier="booking" type="Booking"
				cardinality="many" collectiontype="list">
				<modifiers read="true" write="true" search="true"
					optional="true" partof="true" />
			</targetElement>
		</relation>

		<relation code="FlightToBookings" generate="true"
			localized="false" autocreate="true">
			<sourceElement qualifier="flight" type="Flight"
				cardinality="one">
				<modifiers read="true" write="true" search="true"
					optional="false" />
			</sourceElement>
			<targetElement qualifier="booking" type="Booking"
				cardinality="many" collectiontype="list">
				<modifiers read="true" write="true" search="true"
					optional="true" partof="true" />
			</targetElement>
		</relation>

	</relations>

	<!-- _________________________________ ITEM ____________________________________________ -->

	<itemtypes>

		<itemtype code="Airport" generate="true" autocreate="true">
			<deployment table="airport" typecode="20101" />
			<attributes>
				<attribute qualifier="codeIATA" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="name" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="city"
					type="localized:java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />

				</attribute>
				<attribute qualifier="country"
					type="localized:java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="runways" type="java.lang.Integer">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
			</attributes>
		</itemtype>


		<itemtype code="Flight" generate="true" autocreate="true">
			<deployment table="flight" typecode="20104" />
			<attributes>
				<attribute qualifier="codeFlight" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="scheduleTimeDep"
					type="java.util.Date">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="actualTimeDep" type="java.util.Date">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="stateFlight" type="stateFlightEnum">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="delay" type="java.lang.Integer">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="plane" type="Plane">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
			</attributes>
		</itemtype>


		<itemtype code="AirLine" generate="true" autocreate="true">
			<deployment table="airline" typecode="20102" />
			<attributes>
				<attribute qualifier="codeAirLine" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="name" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Plane" generate="true" autocreate="true">
			<deployment table="plane" typecode="20105" />
			<attributes>
				<attribute qualifier="codePlane" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="typePlane" type="TypePlane">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="statePlane" type="statePlaneEnum">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="TypePlane" generate="true" autocreate="true">
			<deployment table="typeplane" typecode="20106" />
			<attributes>
				<attribute qualifier="model" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="numMaxSits" type="java.lang.Integer">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="goods" type="java.lang.Integer">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Passenger" extends="User" generate="true"
			autocreate="true">
			<deployment table="passenger" typecode="20107" />
			<attributes>
				<attribute qualifier="fiscalCode" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="birthday" type="java.util.Date">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="sex" type="Gender">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="nationality" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
			</attributes>
		</itemtype>


		<itemtype code="Route" generate="true" autocreate="true">
			<deployment table="route" typecode="20103" />
			<attributes>
				<attribute qualifier="codeRoute" type="java.lang.String">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
				<attribute qualifier="airportDep" type="Airport">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="airportArr" type="Airport">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="routeTime" type="java.lang.Integer">
					<persistence type="property"></persistence>
					<modifiers optional="false" />
				</attribute>
			</attributes>
		</itemtype>

		<itemtype code="Booking" generate="true" autocreate="true">
			<deployment table="booking" typecode="20108" />
			<attributes>
				<attribute qualifier="sit" type="java.lang.Integer">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
				<attribute qualifier="bookingDate" type="java.util.Date">
					<persistence type="property"></persistence>
					<modifiers optional="true" />
				</attribute>
			</attributes>
		</itemtype>



		<!-- TypeGroups are for structure of this file only -->

		<typegroup name="Apparel">
			<itemtype code="ApparelProduct" extends="Product"
				autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelProduct">
				<description>Base apparel product extension that contains additional attributes.</description>
				<attributes>
					<attribute qualifier="genders" type="GenderList">
						<description>List of genders that the ApparelProduct is designed for</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>

			<itemtype code="ApparelStyleVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelStyleVariantProduct">
				<description>Apparel style variant type that contains additional attribute describing variant style.
                </description>
				<attributes>
					<attribute qualifier="style"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color/Pattern of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>

					<attribute qualifier="swatchColors" type="SwatchColorSet">
						<description>A normalized color mapping to a standardized front-end navigable name.
                        </description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>

			</itemtype>

			<itemtype code="ApparelSizeVariantProduct"
				extends="ApparelStyleVariantProduct" autocreate="true"
				generate="true"
				jaloclass="it.uiip.airport.core.jalo.ApparelSizeVariantProduct">
				<description>Apparel size variant type that contains additional attribute describing variant size.
                </description>
				<attributes>
					<attribute qualifier="size"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Size of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>

		<typegroup name="Electronics">
			<itemtype code="ElectronicsColorVariantProduct"
				extends="VariantProduct" autocreate="true" generate="true"
				jaloclass="it.uiip.airport.core.jalo.ElectronicsColorVariantProduct">
				<description>Electronics color variant type that contains additional attribute describing variant color.
                </description>
				<attributes>
					<attribute qualifier="color"
						type="localized:java.lang.String"
						metatype="VariantAttributeDescriptor">
						<description>Color of the product.</description>
						<modifiers />
						<persistence type="property" />
					</attribute>
				</attributes>
			</itemtype>
		</typegroup>



	</itemtypes>


</items>
